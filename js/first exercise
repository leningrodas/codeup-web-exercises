var itemsBought = 3;
var didBuyTwo = itemsBought > 2;
var expirationDate = "2020-09-20":
var today = "2020-09-25";
var isExpired = expirationDate <= today;
var isPremium = false;

var canGetDiscount = false;

if(didBuyTwo && !isExpired){
    canGetDiscount = true;
} else if(isPremium && !isExpired){
    canGetDiscount = true;
} else {
    canGetDiscount = false;
}
//above in words says: if they have buy and is not expired than get discount
//if they are premium and is not expired they can get discount
//if neither of the top two are true then they do not get discount


